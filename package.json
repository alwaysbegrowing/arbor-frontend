{
  "name": "arbor-frontend",
  "description": "Arbor protocol frontend.",
  "license": "GPL-3.0-or-later",
  "homepage": "https://arbor.vercel.app/",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "react-app-rewired start",
    "dev": "react-app-rewired start",
    "prebuild": "pnpm run format",
    "build": "pnpm run typechain:all && react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "format:eslint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "format:prettier": "prettier \"src/**/*.{js,jsx,ts,tsx}\" --write",
    "format": "pnpm run format:eslint && pnpm run format:prettier",
    "eject": "react-app-rewired eject",
    "prepare": "husky install",
    "postinstall": "pnpm run typechain:all",
    "typechain:abi": "typechain --target=ethers-v5 ./src/constants/abis/**/*.json --out-dir=./gen/types",
    "typechain:gql": "graphql-codegen --require dotenv/config --config .graphqlrc",
    "typechain:all": "pnpm run typechain:abi && pnpm run typechain:gql"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
